{
  "expo": {
    "name": "google-auth-expo-react-native",
    "slug": "authentication",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-tracking-transparency",
      [
        "react-native-fbsdk-next",
        {
          "appID": "1175821280506368",
          "clientToken": "130b248e3c34de3cf597811e9f4bfdb5",
          "displayName": "hintLogin",
          "scheme": "fb1175821280506368",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.misscoding.googleauthexporeactnative",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.72307864794-1vuq2apibl4tg6on2f2nmoq5vul3ltvq"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.misscoding.googleauthexporeactnative",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e1fe903e-03f1-482e-99ba-a32e9fcab222"
      }
    },
    "owner": "nirupamagarwal"
  }
}
